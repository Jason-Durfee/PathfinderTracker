@model ArmorAddon

<div class="form-group">
    <label asp-for="GPValue" class="control-label"></label>
    <input asp-for="GPValue" class="form-control" />
    <span asp-validation-for="GPValue" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="ArmorCheckPenalty" class="control-label"></label>
    <input asp-for="ArmorCheckPenalty" class="form-control" />
    <span asp-validation-for="ArmorCheckPenalty" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Weight" class="control-label"></label>
    <input asp-for="Weight" class="form-control" />
    <span asp-validation-for="Weight" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="MaterialID" class="control-label"></label>
    <select asp-for="MaterialID" class="form-control" asp-items="ViewBag.MaterialID"></select>
    <span asp-validation-for="MaterialID" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
