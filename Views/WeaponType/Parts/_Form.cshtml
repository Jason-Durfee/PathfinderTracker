@model WeaponType

<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="DamageType" class="control-label"></label>
    <input asp-for="DamageType" class="form-control" />
    <span asp-validation-for="DamageType" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="AttackDiceSmall" class="control-label"></label>
    <input asp-for="AttackDiceSmall" class="form-control" />
    <span asp-validation-for="AttackDiceSmall" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="AttackDiceMedium" class="control-label"></label>
    <input asp-for="AttackDiceMedium" class="form-control" />
    <span asp-validation-for="AttackDiceMedium" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="HasReach" class="control-label"></label>
    <input asp-for="HasReach" class="form-control" />
    <span asp-validation-for="HasReach" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Critical" class="control-label"></label>
    <input asp-for="Critical" class="form-control" />
    <span asp-validation-for="Critical" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="AttackRange" class="control-label"></label>
    <input asp-for="AttackRange" class="form-control" />
    <span asp-validation-for="AttackRange" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="GPValue" class="control-label"></label>
    <input asp-for="GPValue" class="form-control" />
    <span asp-validation-for="GPValue" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Weight" class="control-label"></label>
    <input asp-for="Weight" class="form-control" />
    <span asp-validation-for="Weight" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="WeaponCategory" class="control-label"></label>
    <select asp-for="WeaponCategoryID" class="form-control" asp-items="ViewBag.WeaponCategoryID"></select>
</div>
<div class="form-group">
    <label asp-for="WeaponCoreType" class="control-label"></label>
    <select asp-for="WeaponCoreTypeID" class="form-control" asp-items="ViewBag.WeaponCoreTypeID"></select>
</div>