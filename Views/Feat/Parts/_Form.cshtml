@model Feat

<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="FeatTypeID" class="control-label"></label>
    <select asp-for="FeatTypeID" class="form-control chosen-select" asp-items="ViewBag.FeatTypeID"></select>
</div>
<div class="form-group">
    <label asp-for="Description" class="control-label"></label>
    <input asp-for="Description" class="form-control" />
    <span asp-validation-for="Description" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Prerequisites" class="control-label"></label>
    <input asp-for="Prerequisites" class="form-control" />
    <span asp-validation-for="Prerequisites" class="text-danger"></span>
</div>