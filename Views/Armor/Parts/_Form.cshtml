@model Armor

<div class="form-group">
    <label asp-for="BaseGPValue" class="control-label"></label>
    <input asp-for="BaseGPValue" class="form-control" />
    <span asp-validation-for="BaseGPValue" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="ACBonus" class="control-label"></label>
    <input asp-for="ACBonus" class="form-control" />
    <span asp-validation-for="ACBonus" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="ArmorCheckPenalty" class="control-label"></label>
    <input asp-for="ArmorCheckPenalty" class="form-control" />
    <span asp-validation-for="ArmorCheckPenalty" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Weight" class="control-label"></label>
    <input asp-for="Weight" class="form-control" />
    <span asp-validation-for="Weight" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Material" class="control-label"></label>
    <select asp-for="MaterialID" class="form-control" asp-items="ViewBag.MaterialID"></select>
</div>
<div class="form-group">
    <label asp-for="ArmorType" class="control-label"></label>
    <select asp-for="ArmorTypeID" class="form-control" asp-items="ViewBag.ArmorTypeID"></select>
</div>
<div class="form-group">
    <label asp-for="ArmorCoreType" class="control-label"></label>
    <select asp-for="ArmorCoreTypeID" class="form-control" asp-items="ViewBag.ArmorCoreTypeID"></select>
</div>
<div class="form-group">
    <label asp-for="ArmorAddon" class="control-label"></label>
    <select asp-for="ArmorAddonID" class="form-control" asp-items="ViewBag.ArmorAddonID"></select>
</div>
<div class="form-group">
    <label asp-for="SpecialAttributes" class="control-label"></label>
    <input asp-for="SpecialAttributes" class="form-control" />
    <span asp-validation-for="SpecialAttributes" class="text-danger"></span>
</div>
