@model Armor

<div class="form-group">
    <label asp-for="Material" class="control-label"></label>
    <select asp-for="MaterialID" class="form-control chosen-select" asp-items="ViewBag.MaterialID"></select>
</div>
<div class="form-group">
    <label asp-for="ArmorType" class="control-label"></label>
    <select asp-for="ArmorTypeID" class="form-control chosen-select" asp-items="ViewBag.ArmorTypeID"></select>
</div>
<div class="form-group">
    <label asp-for="ArmorAddon" class="control-label"></label>
    <select asp-for="ArmorAddonID" class="form-control chosen-select" asp-items="ViewBag.ArmorAddonID"></select>
</div>
<div class="form-group">
    <label asp-for="SpecialAttributes" class="control-label"></label>
    <input asp-for="SpecialAttributes" class="form-control" />
    <span asp-validation-for="SpecialAttributes" class="text-danger"></span>
</div>
